<IfModule mod_authz_core.c>
  <FilesMatch ".*">
    Require all denied
  </FilesMatch>
  <FilesMatch "(?i)\.(?:jpe?g|png|gif|webp|avif|svg)$">
    Require all granted
  </FilesMatch>
</IfModule>

<IfModule !mod_authz_core.c>
  <FilesMatch ".*">
    Deny from all
  </FilesMatch>
  <FilesMatch "(?i)\.(?:jpe?g|png|gif|webp|avif|svg)$">
    Allow from all
  </FilesMatch>
</IfModule>
